@model List<DemoProject.Models.Setting>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Key</th>
            <th>Value</th>
            <th>Value2</th>
            <th>Description</th>
            <th>Created</th>
            <th>Last Modified</th>
            <th>Is Deleted</th>
            <th>Edit/Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var setting in Model)
        {
            <tr>
                <td>@setting.Id</td>
                <td>@setting.Key</td>
                <td>@setting.Value</td>
                <td>@setting.Value2</td>
                <td>@setting.Description</td>
                <td>@setting.Created</td>
                <td>@setting.LastModified</td>
                <td>@setting.IsDeleted</td>
                <td>
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="Setting" asp-action="Edit" asp-route-id="@setting.Id" class="btn btn-primary mx-2">
                            <i class="bi bi-pencil-square"></i>Edit
                        </a>
                        <a asp-controller="Setting" asp-action="Delete" asp-route-id="@setting.Id" class="btn btn-danger mx-2">
                            <i class="bi bi-trash-fill"></i>Delete
                        </a>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>
<a type="button" asp-controller="Setting" asp-action="Create" class="btn btn-primary">Create</a>

